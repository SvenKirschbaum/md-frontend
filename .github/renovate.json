{
  "extends": [
    "github>fallobst22/renovate-config"
  ],
  "packageRules": [
    {
      "matchPackageNames": ["react-router", "react-router-dom"],
      "allowedVersions": "<6.0.0"
    },
    {
      "matchPackageNames": [
        "react-router-bootstrap"
      ],
      "allowedVersions": "<0.26.0"
    },
    {
      "matchPackageNames": ["react-bootstrap"],
      "allowedVersions": "<2.0.0"
    },
    {
      "matchPackageNames": [
        "react",
        "react-dom",
        "@types/react",
        "@types/react-dom"
      ],
      "allowedVersions": "<18.0.0"
    },
    {
      "matchPackageNames": [
        "@testing-library/react"
      ],
      "allowedVersions": "<13.0.0"
    },
    {
      "matchPackageNames": [
        "react-data-table-component"
      ],
      "allowedVersions": "<7.0.0"
    }
  ]
}
